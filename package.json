{
  "name": "ngx-soundmanager2",
  "version": "0.0.0",
  "description": "SoundManager2 Music Player for Angular",
  "main": "ngx-soundmanager2.bundle.js",
  "jsnext:main": "ngx-soundmanager2.module.js",
  "module": "ngx-soundmanager2.module.js",
  "types": "ngx-soundmanager2.module.d.ts",
  "scripts": {
    "test": "mocha --require ts-node/register test/**/*.spec.ts --recursive",
    "test:watch": "mocha -w --require ts-node/register test/**/*.spec.ts --recursive",
    "build": "rimraf dist && tsc -p tsconfig-esm.json && rollup -c rollup.config.js dist/ngx-soundmanager2.module.js > dist/ngx-soundmanager2.bundle.js && cp package.json dist && ts-node tools/cleanup.ts && ngc && cp README.md dist",
    "postbuild": "",
    "build:demo": "rimraf demo/dist && tsc -p tsconfig-demo.json",
    "lint": "tslint -c tslint.json lib/**/*.ts test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lfarran/ngx-soundmanager2.git"
  },
  "keywords": [
    "angular",
    "soundmanager2",
    "music",
    "audio",
    "player",
    "music player",
    "audio player"
  ],
  "author": "Luke Farran <lfarran>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lfarran/ngx-soundmanager2/issues"
  },
  "homepage": "https://github.com/lfarran/ngx-soundmanager2#readme",
  "peerDependencies": {
    "@angular/core": ">=4.0.0"
  },
  "devDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.37",
    "@types/node": "^7.0.0",
    "chai": "^3.5.0",
    "codelyzer": "^3.1.2",
    "core-js": "^2.4.1",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.43.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rxjs": "5.4.2",
    "soundmanager2": "^2.97.20170602",
    "systemjs": "0.20.14",
    "ts-helpers": "^1.1.2",
    "ts-node": "^3.0.4",
    "tslib": "^1.5.0",
    "tslint": "^5.0.0",
    "typescript": "^2.1.5",
    "uglifyjs": "^2.4.10",
    "zone.js": "^0.8.4"
  }
}